{
  "name": "bricks-minesweeper",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test --modulePaths=src",
    "test:debug": "react-app-rewired --inspect-brk test --runInBand --no-cache",
    "test:snaps": "react-app-rewired test --updateSnapshot",
    "eject": "react-scripts eject",
    "test:local": "npm test --colors --bail",
    "test:deploy": "npm test --coverage --colors --bail",
    "test:ci": "CI=true npm run test:deploy",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook",
    "pretty:all": "prettier **/*.{ts,tsx,js,jsx,json} --write",
    "pretty:quick": "pretty-quick",
    "lint:js": "eslint src/**/*.{ts,tsx}",
    "lint:js:fix": "npm run lint:js -- --fix",
    "rename-js": "for f in ./build/static/js/*.gz; do mv -- \"$f\" \"${f%.gz}\";  done",
    "rename-css": "for f in ./build/static/css/*.gz; do mv -- \"$f\" \"${f%.gz}\";  done",
    "ci:build": "aws codebuild start-build --project-name ccms-ui"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "npm run lint:js:fix",
      "git add"
    ]
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/testing/utils/assetsTransformer.ts",
      "\\.(css|less)$": "<rootDir>/src/testing/utils/assetsTransformer.ts"
    }
  },
